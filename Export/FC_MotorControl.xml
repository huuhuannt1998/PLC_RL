      <SW.Blocks.CompileUnit ID="1" CompositionName="CompileUnits">
        <AttributeList>
          <NetworkSource><StructuredText xmlns="http://www.siemens.com/automation/Openness/SW/NetworkSource/StructuredText/v3">
// Motor Control Logic
IF #Start AND NOT #Stop THEN
    #Motor_On := TRUE;
    #temp_speed := #Speed_Setpoint;
    
    // Ramp up speed
    IF #Current_Speed &lt; #temp_speed THEN
        #Current_Speed := #Current_Speed + 10.0;
        IF #Current_Speed &gt; #temp_speed THEN
            #Current_Speed := #temp_speed;
        END_IF;
    END_IF;
ELSE
    #Motor_On := FALSE;
    // Ramp down speed
    IF #Current_Speed &gt; 0.0 THEN
        #Current_Speed := #Current_Speed - 10.0;
        IF #Current_Speed &lt; 0.0 THEN
            #Current_Speed := 0.0;
        END_IF;
    END_IF;
END_IF;
</StructuredText></NetworkSource>
          <ProgrammingLanguage>SCL</ProgrammingLanguage>
